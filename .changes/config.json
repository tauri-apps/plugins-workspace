{
  "gitSiteUrl": "https://github.com/tauri-apps/plugins-workspace/",
  "pkgManagers": {
    "javascript": {
      "version": true,
      "getPublishedVersion": "pnpm view ${ pkgFile.pkg.name } version",
      "prepublish": ["pnpm build"],
      "publish": ["pnpm publish --access public --no-git-checks"]
    },
    "rust": {
      "version": true,
      "getPublishedVersion": "cargo search ${ pkgFile.pkg.package.name } --limit 1 | sed -nE 's/^[^\"]*\"//; s/\".*//1p' -",
      "publish": [
        {
          "command": "cargo package --no-verify",
          "dryRunCommand": true
        },
        {
          "command": "echo '<details>\n<summary><em><h4>Cargo Publish</h4></em></summary>\n\n```'",
          "dryRunCommand": true,
          "pipe": true
        },
        {
          "command": "cargo publish",
          "dryRunCommand": "cargo publish --dry-run",
          "pipe": true
        },
        {
          "command": "echo '```\n\n</details>\n'",
          "dryRunCommand": true,
          "pipe": true
        }
      ],
      "postpublish": [
        "git tag ${ pkgFile.pkg.package.name }-v${ pkgFile.versionMajor } -f",
        "git tag ${ pkgFile.pkg.package.name }-v${ pkgFile.versionMajor }.${ pkgFile.versionMinor } -f",
        "git push --tags -f"
      ]
    }
  },
  "packages": {
    "authenticator": {
      "path": "./plugins/authenticator",
      "manager": ""
    },
    "authenticator-js": {
      "path": "./plugins/authenticator",
      "manager": ""
    },

